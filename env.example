# ───────────────────────────────────────────────────────────
# 🔒 Bot Configuration
# ───────────────────────────────────────────────────────────

# Bot Token (Required)
BOT_TOKEN=your_bot_token_here

# Mongo Database Connection String (Required)
MONGO_CONNECTION=your_mongo_connection_string_here

# ───────────────────────────────────────────────────────────
# 🌐 Webhooks (Optional)
# ───────────────────────────────────────────────────────────

ERROR_LOGS=your_error_logs_webhook_here
JOIN_LEAVE_LOGS=your_join_leave_logs_webhook_here

# ───────────────────────────────────────────────────────────
# 🛠️ Dashboard Configuration (Required for Dashboard)
# ───────────────────────────────────────────────────────────

BOT_SECRET=your_bot_secret_here
SESSION_PASSWORD=your_session_password_here

# ───────────────────────────────────────────────────────────
# 🌦️ Weather API (Required for Weather Command)
# Sign up at: https://weatherstack.com
# ───────────────────────────────────────────────────────────

WEATHERSTACK_KEY=your_weatherstack_key_here

# ───────────────────────────────────────────────────────────
# 🖼️ Image API (Required for Image Commands)
# API Docs: https://strangeapi.fun/docs
# ───────────────────────────────────────────────────────────

STRANGE_API_KEY=your_strangeapi_key_here

# ───────────────────────────────────────────────────────────
# 🎵 Spotify API (Required for Spotify Support)
# Developer Portal: https://developer.spotify.com
# ───────────────────────────────────────────────────────────

SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
